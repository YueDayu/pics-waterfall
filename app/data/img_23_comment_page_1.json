[{"comment": "\u4f60\u82e5\u8d50\u6211\u4e00\u6bb5\u6d6e\u534e\uff0c\u6211\u4fbf\u8bb8\u4f60\u6ee1\u4e16\u7e41\u82b1", "name": "\u67ef\u8c6a"}, {"comment": "\u6211\u73b0\u5728\u4f9d\u7136\u559c\u6b22\u7740\u4f60\uff0c\u4f46\u5c31\u7b97\u6211\u4eec\u6765\u5f80\u4e00\u5343\u5c01\u77ed\u4fe1\uff0c\u5fc3\u5374\u4e0d\u8fc7\u63a5\u8fd1\u4ec5\u4ec5\u4e00\u5398\u7c73\u3002", "name": "\u6850\u5988"}, {"comment": "\u671b\u5411\u955c\u4e2d\u7684\u81ea\u5df1\uff0c\u6c34\u6c7d\u5f25\u6f2b\u3002", "name": "\u738b\u662d"}, {"comment": "\u4eca\u5929\u7684\u98ce\u513f\u6709\u4e9b\u55a7\u56a3\u5440", "name": "\u5f20\u51ef"}, {"comment": "\u4e00\u5207\u7684\u4e00\u5207\u5c31\u50cf\u4e00\u4e2a\u4f20\u8bf4,\u6211\u662f\u90a3\u4f20\u8bf4\u4e2d\u7684\u738b\u5b50,\u6253\u9a6c\u4ece\u57a3\u4e0a\u63a0\u8fc7", "name": "\u7f57\u5251\u6865"}, {"comment": "\u7b11\u662f\u5bf9\u8eab\u8fb9\u7684\u4e8b\u7269\u611f\u5230\u4e00\u77ac\u95f4\u7684\u5e78\u798f\u800c\u8868\u73b0\u51fa\u6765\u7684\u4e1c\u897f\u3002", "name": "\u5927\u68cd"}, {"comment": "\u7b11\u662f\u5bf9\u8eab\u8fb9\u7684\u4e8b\u7269\u611f\u5230\u4e00\u77ac\u95f4\u7684\u5e78\u798f\u800c\u8868\u73b0\u51fa\u6765\u7684\u4e1c\u897f\u3002", "name": "\u674e\u6839"}, {"comment": "\u4f60\u7ad9\u7684\u65b9\u5411\u5439\u8fc7\u6765\u7684\u98ce\u662f\u6696\u7684", "name": "\u5f20\u51ef"}, {"comment": "\u8fd9\u4e2a\u4e16\u754c\u4e0a\u6700\u63a5\u8fd1\u6076\u9b54\u7684\u751f\u7269\u5e94\u8be5\u5c31\u662f\u4eba\u7c7b\u4e86\u3002", "name": "\u9ec4\u7816"}, {"comment": "\u4e0d\u60f3\u54ed\u5c31\u53ea\u6709\u53d8\u5f3a\u4e86\uff0c\u65e0\u8bba\u53d1\u751f\u4ec0\u4e48\u90fd\u4e0d\u4f1a\u6389\u6cea\u7684\u575a\u5f3a\u3002", "name": "\u5927\u5e1d"}]